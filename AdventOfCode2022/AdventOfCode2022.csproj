<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <AssemblyName>AdventOfCode2022</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
	<Folder Include="Views\" />
	<Folder Include="Views\Days\" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="DefaultInputs.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>DefaultInputs.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="DefaultInputs.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>DefaultInputs.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
